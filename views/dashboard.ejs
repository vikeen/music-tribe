<div class="container">
  <h1>This is the user's dashboard</h1>

  <div class="row">
    <div class="col-xs-12">
      <h2>Upload Assets</h2>
      <form class="form" method="POST" action="/file" enctype="multipart/form-data">
        <div class="form-group">
          <input id="song" name="song" type="file" placeholder="Add Song" multiple="multiple">
        </div>
        <div class="form-group">
          <button type="submit" class="btn btn-primary">Upload</button>
        </div>
      </form>
    </div>
    <section class="col-xs-12 asset-list">
      <h2>Current Assets</h2>
      <% if (artistsAssets.length) { %>
      <% for(var i = 0; i < artistsAssets.length; i++) { %>
      <% var asset = artistsAssets[i]; %>
      <% if(i % 4 === 0) { %>
      <%- '<div class="row">' %>
      <% } %>
      <div class="col-xs-12 col-sm-6 col-md-3">
        <%- include artist-asset.ejs %>
      </div>
      <% if(i % 4 === 3) { %>
      <%- '</div>' %>
      <% } %>
      <% } %>
      <% } %>
    </section>
  </div>
</div>
